{
  "name": "@7kprotocol/sdk-ts",
  "version": "3.6.1-beta.2",
  "description": "",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prepare": "husky",
    "tsc": "npx tsc -p ./configs/tsconfig.base.json --noEmit",
    "lint": "npx eslint ./src",
    "prettier": "npx prettier --write .",
    "build": "tsup",
    "watch": "tsup --watch",
    "prepack": "npm run build",
    "test": "mocha --timeout 60000",
    "pack:local": "npm run build && npm pack",
    "publish:patch": "npm version patch --no-git-tag-version && npm publish --access public",
    "publish:minor": "npm version minor --no-git-tag-version && npm publish --access public",
    "publish:major": "npm version major --no-git-tag-version && npm publish --access public",
    "publish:beta": "npm version prerelease --preid=beta --no-git-tag-version && npm publish --tag beta --access public"
  },
  "keywords": [
    "7k Protocol",
    "DEX Aggregator",
    "Sui Network"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/7k-ag/7k-sdk-ts"
  },
  "author": "7k Protocol",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/chai": "^5.2.2",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.10",
    "chai": "^5.2.0",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "mocha": "^11.6.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "peerDependencies": {
    "@mysten/sui": "^1.44.0",
    "@pythnetwork/pyth-sui-js": "^2.2.0"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  },
  "optionalDependencies": {
    "@bluefin-exchange/bluefin7k-aggregator-sdk": "^5.1.4",
    "@cetusprotocol/aggregator-sdk": "^1.4.1",
    "@flowx-finance/sdk": "^1.13.8"
  }
}
